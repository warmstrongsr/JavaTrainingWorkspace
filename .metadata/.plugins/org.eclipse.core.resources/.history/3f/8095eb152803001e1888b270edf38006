
public class WhileLoop {

	public static void main(String[] args) {
		loop();

	}

	static void loop() {
		boolean running = false;
		while(running) {
			System.out.println("Running");
		}
	}
	static boolean check()	{
		if(Math.random() < 0.5) {
			return false;
			
		}else {
			return true;
		}
	}
}
